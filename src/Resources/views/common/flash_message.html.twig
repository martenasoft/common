
{% for message in app.flashes(constant('MartenaSoft\\Common\\Library\\CommonValues::FLASH_SUCCESS_TYPE')) %}
    <div class="alert alert-success">
        {{ message }}
    </div>
{% endfor %}

{% for message in app.flashes(constant('MartenaSoft\\Common\\Library\\CommonValues::FLASH_ERROR_TYPE')) %}
    <div class="alert alert-danger">
        {{ message }}
    </div>
{% endfor %}